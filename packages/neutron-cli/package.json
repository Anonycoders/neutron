{
  "name": "@neutronjs/cli",
  "description": "This CLI was developed to help developers create new projects quickly and easily",
  "version": "4.0.0-next.1",
  "license": "MIT",
  "main": "build/cli.js",
  "bin": {
    "neutron": "bin/neutron"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,jsx,json} && eslint **/*.{js,jsx} --fix",
    "precommit": "lint-staged"
  },
  "author": {
    "name": "Thiago de Souza",
    "email": "dev@thiagodesouza.com",
    "url": "https://github.com/thiagodesouza"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neutronjs/neutron.git"
  },
  "keywords": [
    "neutron",
    "expo",
    "react",
    "react-native",
    "node",
    "javascript",
    "gluegun"
  ],
  "files": [
    "src",
    "LICENSE",
    "README.md",
    "bin"
  ],
  "lint-staged": {
    "**/*.{js,jsx}": [
      "yarn format",
      "git add ."
    ]
  },
  "dependencies": {
    "exec-sh": "^0.3.4",
    "gluegun": "^4.1.2",
    "hyperquest": "^2.1.3",
    "tar-pack": "^3.4.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  }
}
