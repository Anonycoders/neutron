{
  "name": "@neutronjs/cli",
  "description": "This CLI was developed to help developers create new projects quickly and easily",
  "version": "3.0.1",
  "license": "MIT",
  "main": "build/cli.js",
  "bin": {
    "neutron": "bin/neutron"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,jsx,json} && eslint **/*.{js,jsx} --fix"
  },
  "author": {
    "name": "Thiago de Souza",
    "email": "dev@thiagodesouza.com",
    "url": "https://github.com/thiagodesouza"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neutronjs/neutron.git"
  },
  "keywords": [
    "neutron",
    "expo",
    "react",
    "react-native",
    "node",
    "javascript",
    "gluegun"
  ],
  "files": [
    "src",
    "LICENSE",
    "README.md",
    "bin"
  ],
  "lint-staged": {
    "./**/*.js": [
      "yarn format",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "exec-sh": "^0.3.4",
    "gluegun": "^4.1.2",
    "hyperquest": "^2.1.3",
    "tar-pack": "^3.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  }
}
