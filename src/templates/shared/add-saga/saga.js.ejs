import { call, put } from 'redux-saga/effects';
import api from '~/services/api';

import <%= props.pascalCaseName %>Actions from '../ducks/<%= props.camelCaseName %>';

export function* get<%= props.pascalCaseName %>Request() {
  try {
    const endpoint = '/<%= props.camelCaseName %>';
    const { data } = yield call(api.get, endpoint);
    yield put(<%= props.pascalCaseName %>Actions.get<%= props.pascalCaseName %>Success(data));
  } catch (error) {
    console.log(error);
    yield put(<%= props.pascalCaseName %>Actions.get<%= props.pascalCaseName %>Failure());
  }
}
